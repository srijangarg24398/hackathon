<!DOCTYPE html>
<html>
<head>
  <title>Tryol</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- <script src="https://use.fontawesome.com/68872fb2de.js"></script> -->
    <!-- <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css"> -->

  <%= csrf_meta_tags %>
</head>
<body>
<% if notice %>
<script type="text/javascript">
	noty({text:"<%= notice %>",type:'alert'});
</script>
<% end %>
<% if alert %>
<script type="text/javascript">
	noty({text:"<%= alert %>",type:'error'});
</script>
<% end %>


            <% if customer_signed_in? %>
              <%= link_to "Sign Out",destroy_customer_session_path,method: :delete,:class=>"dropdown-item" %>
            <% end %>
            <% if retailer_signed_in? %>
              <%= link_to "Sign Out",destroy_retailer_session_path,method: :delete,:class=>"dropdown-item" %>
            <% end %>
            <% if manufacturer_signed_in? %>
              <%= link_to "Sign Out",destroy_manufacturer_session_path,method: :delete,:class=>"dropdown-item" %>
            <% end %>
            <% if restaurant_signed_in? %>
              <%= link_to "Sign Out",destroy_restaurant_session_path,method: :delete,:class=>"dropdown-item" %>
            <% end %>

<%= yield %>

</body>
</html>
